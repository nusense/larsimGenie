BEGIN_PROLOG

genie_eventweight:
{
  module_type:	"EventWeight"

  weight_functions: [ MaCCQE ]

  # Throw exception if weight is outside these limits
  min_weight: 0
  max_weight: 100

  genie_module_label:    generator	

  # CCQE axial mass for dipole form factor
  MaCCQE: {
    type: Genie
    random_seed: 1
    parameter_list: [ "MaCCQE" ] 
    parameter_sigma: [ 1 ] 
    mode: pm1sigma
    number_of_multisims: 2
  }
}	

END_PROLOG
